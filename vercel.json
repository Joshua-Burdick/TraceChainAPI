{
    "version": 2,
    "builds": [
        {
        "src": "/api/**/*.js",
        "use": "@vercel/node"
        },
        {
        "src": "/*.js",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/api/.*",
        "dest": "/api/**/*.js"
        },
        {
        "src": "/(.*)",
        "dest": "/index.js"
        }
    ]
}
  